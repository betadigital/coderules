{
  "welcomeFile": "/index.html",
  "routes": [
    {
      "source": "^/baseruleapp/(.*)$",
      "target": "$1",
      "localDir": "../baseruleapp/webapp",
      "authenticationType": "xsuaa"
    },
    {
      "source": "^/userrulesapp/(.*)$",
      "target": "$1",
      "localDir": "../userrulesapp/webapp",
      "authenticationType": "xsuaa"
    },
    {
      "source": "^/logsapp/(.*)$",
      "target": "$1",
      "localDir": "../logsapp/webapp",
      "authenticationType": "xsuaa"
    },
    {
      "source": "^/objecttypes/(.*)$",
      "target": "$1",
      "localDir": "../objecttypes/webapp",
      "authenticationType": "xsuaa"
    },
    {
      "source": "^/resources/(.*)$",
      "target": "/resources/$1",
      "authenticationType": "none",
      "destination": "sapui5"
    },
    {
      "source": "^/test-resources/(.*)$",
      "target": "/test-resources/$1",
      "authenticationType": "none",
      "destination": "sapui5"
    },
    {
      "source": "^/codeRuleService/(.*)$",
      "target": "/codeRuleService/$1",
      "destination": "srv-api",
      "authenticationType": "none",
      "csrfProtection": false
    },
    {
      "source": "^/baseRuleService/(.*)$",
      "target": "/baseRuleService/$1",
      "destination": "srv-api",
      "authenticationType": "none",
      "csrfProtection": false
    },
    {
      "source": "^/objectTypeService/(.*)$",
      "target": "/objectTypeService/$1",
      "destination": "srv-api",
      "authenticationType": "none",
      "csrfProtection": false
    },
    {
      "source": "^/(.*)$",
      "target": "$1",
      "localDir": "webapp",
      "authenticationType": "xsuaa"
    }
  ]
}
